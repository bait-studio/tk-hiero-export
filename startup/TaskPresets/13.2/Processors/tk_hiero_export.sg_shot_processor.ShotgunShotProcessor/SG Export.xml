<root presetname="SG Export" tasktype="tk_hiero_export.sg_shot_processor.ShotgunShotProcessor">
    <excludeTags valuetype="list" />
    <includeTags valuetype="list">
        <SequenceItem valuetype="str">Blue</SequenceItem>
    </includeTags>
    <versionIndex valuetype="int">1</versionIndex>
    <versionPadding valuetype="int">2</versionPadding>
    <exportTemplate valuetype="list">
        <SequenceItem valuetype="tuple">
            <SequenceItem valuetype="str">sequences/{sequence}/{shot}/editorial/plates/v{tk_version}/{shot}.####.{fileext}</SequenceItem>
            <SequenceItem valuetype="tk_hiero_export.sg_copy_exporter.ShotgunCopyPreset">
                <root presetname="tk_hiero_export.sg_copy_exporter.ShotgunCopyExporter" tasktype="tk_hiero_export.sg_copy_exporter.ShotgunCopyExporter">
                    <collateTracks valuetype="bool">False</collateTracks>
                    <collateShotNames valuetype="bool">False</collateShotNames>
                    <collateSequence valuetype="bool">False</collateSequence>
                    <collateCustomStart valuetype="bool">True</collateCustomStart>
                    <create_version valuetype="bool">True</create_version>
                </root>
            </SequenceItem>
        </SequenceItem>
        <SequenceItem valuetype="tuple">
            <SequenceItem valuetype="str">sequences/{sequence}/{shot}/work/nuke/{shot}_Comp.v{tk_version}.nk</SequenceItem>
            <SequenceItem valuetype="tk_hiero_export.sg_nuke_shot_export.ShotgunNukeShotPreset">
                <root presetname="" tasktype="tk_hiero_export.sg_nuke_shot_export.ShotgunNukeShotExporter">
                    <enable valuetype="bool">True</enable>
                    <readPaths valuetype="list">
                        <SequenceItem valuetype="str">sequences/{sequence}/{shot}/editorial/plates/v{tk_version}/{shot}.####.{fileext}</SequenceItem>
                    </readPaths>
                    <writePaths valuetype="list" />
                    <timelineWriteNode valuetype="str" />
                    <collateTracks valuetype="bool">False</collateTracks>
                    <collateShotNames valuetype="bool">False</collateShotNames>
                    <annotationsPreCompPaths valuetype="list" />
                    <includeAnnotations valuetype="bool">False</includeAnnotations>
                    <showAnnotations valuetype="bool">True</showAnnotations>
                    <includeEffects valuetype="bool">True</includeEffects>
                    <connectTracks valuetype="bool">False</connectTracks>
                    <collateSequence valuetype="bool">False</collateSequence>
                    <collateCustomStart valuetype="bool">True</collateCustomStart>
                    <additionalNodesEnabled valuetype="bool">False</additionalNodesEnabled>
                    <additionalNodesData valuetype="list" />
                    <method valuetype="str">Blend</method>
                    <reformat valuetype="dict">
                        <to_type valuetype="str">Plate Resolution</to_type>
                        <resize valuetype="str">width</resize>
                        <center valuetype="bool">True</center>
                        <filter valuetype="str">Cubic</filter>
                    </reformat>
                    <postProcessScript valuetype="bool">True</postProcessScript>
                    <toolkitWriteNodes valuetype="list">
                        <SequenceItem valuetype="str">Toolkit Node: EXR (&quot;exr&quot;)</SequenceItem>
                    </toolkitWriteNodes>
                </root>
            </SequenceItem>
        </SequenceItem>
        <SequenceItem valuetype="tuple">
            <SequenceItem valuetype="str">sequences/{sequence}/{shot}/work/nuke/renders/v{tk_version}/{shot}_comp.####.exr</SequenceItem>
            <SequenceItem valuetype="hiero.exporters.FnExternalRender.NukeRenderPreset">
                <root presetname="" tasktype="hiero.exporters.FnExternalRender.NukeRenderTask">
                    <file_type valuetype="str">exr</file_type>
                    <reformat valuetype="dict">
                        <to_type valuetype="str">None</to_type>
                        <scale valuetype="float">1.0</scale>
                        <resize valuetype="str">width</resize>
                        <center valuetype="bool">True</center>
                        <filter valuetype="str">Cubic</filter>
                    </reformat>
                    <colourspace valuetype="str">default</colourspace>
                    <channels valuetype="str">all</channels>
                    <views valuetype="list">
                        <SequenceItem valuetype="str">all</SequenceItem>
                    </views>
                    <dpx valuetype="dict">
                        <datatype valuetype="str">10 bit</datatype>
                        <fill valuetype="bool">False</fill>
                        <bigEndian valuetype="bool">True</bigEndian>
                        <transfer valuetype="str">(auto detect)</transfer>
                    </dpx>
                    <writeNodeName valuetype="str">Write_{ext}</writeNodeName>
                    <burninDataEnabled valuetype="bool">False</burninDataEnabled>
                    <burninData valuetype="dict">
                        <burnIn_textSize valuetype="NoneType">None</burnIn_textSize>
                        <burnIn_padding valuetype="NoneType">None</burnIn_padding>
                        <burnIn_font valuetype="NoneType">None</burnIn_font>
                        <burnIn_topLeft valuetype="NoneType">None</burnIn_topLeft>
                        <burnIn_topMiddle valuetype="NoneType">None</burnIn_topMiddle>
                        <burnIn_topRight valuetype="NoneType">None</burnIn_topRight>
                        <burnIn_bottomLeft valuetype="NoneType">None</burnIn_bottomLeft>
                        <burnIn_bottomMiddle valuetype="NoneType">None</burnIn_bottomMiddle>
                        <burnIn_bottomRight valuetype="NoneType">None</burnIn_bottomRight>
                    </burninData>
                    <create_directories valuetype="bool">True</create_directories>
                    <exr valuetype="dict">
                        <datatype valuetype="str">16 bit half</datatype>
                        <compression valuetype="str">none</compression>
                        <dw_compression_level valuetype="float">45.0</dw_compression_level>
                        <write_ACES_compliant_EXR valuetype="bool">False</write_ACES_compliant_EXR>
                        <metadata valuetype="str">default metadata</metadata>
                        <noprefix valuetype="bool">False</noprefix>
                        <interleave valuetype="str">channels, layers and views</interleave>
                        <standard_layer_name_format valuetype="bool">False</standard_layer_name_format>
                        <write_full_layer_names valuetype="bool">False</write_full_layer_names>
                        <truncateChannelNames valuetype="bool">False</truncateChannelNames>
                    </exr>
                </root>
            </SequenceItem>
        </SequenceItem>
    </exportTemplate>
    <exportRoot valuetype="str"></exportRoot>
    <cutHandles valuetype="int">12</cutHandles>
    <cutUseHandles valuetype="bool">False</cutUseHandles>
    <cutLength valuetype="bool">True</cutLength>
    <includeRetimes valuetype="bool">False</includeRetimes>
    <startFrameIndex valuetype="int">1001</startFrameIndex>
    <startFrameSource valuetype="str">Custom</startFrameSource>
    <shotgunShotCreateProperties valuetype="dict">
        <collateTracks valuetype="bool">False</collateTracks>
        <collateShotNames valuetype="bool">False</collateShotNames>
        <collateSequence valuetype="bool">False</collateSequence>
        <collateCustomStart valuetype="bool">True</collateCustomStart>
        <sg_status_hiero_tags valuetype="list">
            <SequenceItem valuetype="tuple">
                <SequenceItem valuetype="str">Ready To Start</SequenceItem>
                <SequenceItem valuetype="str">ip</SequenceItem>
            </SequenceItem>
            <SequenceItem valuetype="tuple">
                <SequenceItem valuetype="str">In Progress</SequenceItem>
                <SequenceItem valuetype="str">ip</SequenceItem>
            </SequenceItem>
            <SequenceItem valuetype="tuple">
                <SequenceItem valuetype="str">On Hold</SequenceItem>
                <SequenceItem valuetype="str">wtg</SequenceItem>
            </SequenceItem>
            <SequenceItem valuetype="tuple">
                <SequenceItem valuetype="str">Final</SequenceItem>
                <SequenceItem valuetype="str">fin</SequenceItem>
            </SequenceItem>
        </sg_status_hiero_tags>
        <task_template_map valuetype="list">
            <SequenceItem valuetype="tuple">
                <SequenceItem valuetype="str">Red</SequenceItem>
                <SequenceItem valuetype="str">Bait - Roto &amp; Comp</SequenceItem>
            </SequenceItem>
            <SequenceItem valuetype="tuple">
                <SequenceItem valuetype="str">Blue</SequenceItem>
                <SequenceItem valuetype="str">Bait - Comp Only Shot</SequenceItem>
            </SequenceItem>
        </task_template_map>
        <sg_cut_type valuetype="str">Final</sg_cut_type>
    </shotgunShotCreateProperties>
</root>
